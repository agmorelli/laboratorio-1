#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int i;
	int num;
	int acum=0;
	int promedio;
	
	for(i=0;i<5;i++){
		printf("Ingrese numero: ");
		scanf("%d",&num);
		acum=acum+num;
		
	}
	promedio=acum/i;
	
	printf("el promedio es: %d", promedio);
	
	
	
	return 0;
}
