#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	
	int i;
	int resultado;
	int num;
	
	printf("ingrese un numero: ");
	scanf("%d",&num);
	
	system("cls");
	
	for(i=0;i<11;i++){
		
		resultado=num*i;
		printf("%d x %d = %d\n", num, i, resultado);
		
	}
	system("pause");
	return 0;
}
